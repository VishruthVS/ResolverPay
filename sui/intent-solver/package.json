{
  "name": "intent-protocol-sdk",
  "version": "2.0.0",
  "description": "TypeScript SDK and Solver Engine for Intent Protocol V2 on Sui",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"**/*.ts\"",
    "solver": "ts-node src/solver-engine.ts",
    "api": "ts-node src/api-server.ts",
    "example": "ts-node src/examples.ts"
  },
  "keywords": [
    "sui",
    "blockchain",
    "intent",
    "swap",
    "dex",
    "solver",
    "defi"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@mysten/deepbook-v3": "^1.0.0",
    "@mysten/sui.js": "^0.50.0",
    "cors": "^2.8.6",
    "dotenv": "^16.0.3",
    "express": "^4.22.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.25",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
